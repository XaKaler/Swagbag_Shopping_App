<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/screen_horizontal_padding"
    android:background="@drawable/rec_white_5">

    <TextView
        android:textColor="@color/day_dark_liver_night_white"
        android:id="@+id/textView158"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delivery Charges"
        style="@style/roboto_medium_16_davys_grey"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView57"
        android:layout_width="13dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_cross_sign_without_outline"
        app:tint="@color/day_dark_liver_night_white" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15"
        android:paddingVertical="@dimen/dimen_10"
        android:background="@drawable/rec_dark_white_5"
        android:paddingHorizontal="@dimen/dimen_15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView158">

        <TextView
            android:textColor="@color/day_dark_liver_night_white"
            android:id="@+id/textView164"
            style="@style/roboto_medium_14_davys_grey"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="What is a Delivery Charges?"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView165"
            android:textColor="@color/day_dark_liver_night_Spanish_Gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="“Delivery Charges” is a service charge levied by Swagbag Designs Pvt. Ltd. on low value orders on the Swagbag Platform."
            style="@style/roboto_normal_12_davys_grey"
            android:layout_marginTop="@dimen/dimen_10"
            app:layout_constraintStart_toStartOf="@+id/textView164"
            app:layout_constraintTop_toBottomOf="@+id/textView164" />

        <TextView
            android:id="@+id/textView166"
            android:textColor="@color/day_dark_liver_night_Spanish_Gray"
            style="@style/roboto_normal_12_davys_grey"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15"
            android:text="Have a question? Refer "
            app:layout_constraintStart_toStartOf="@+id/textView165"
            app:layout_constraintTop_toBottomOf="@+id/textView165" />

        <TextView
            android:id="@+id/faqs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FAQ’s"
            style="@style/roboto_medium_12_red_light"
            app:layout_constraintBottom_toBottomOf="@+id/textView166"
            app:layout_constraintStart_toEndOf="@+id/textView166"
            app:layout_constraintTop_toTopOf="@+id/textView166" />

        <View
            android:id="@+id/view35"
            style="@style/line_horizontal"
            android:layout_marginTop="@dimen/dimen_10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/faqs" />

        <TextView
            android:id="@+id/textView168"
            android:textColor="@color/day_dark_liver_night_Spanish_Gray"
            style="@style/roboto_normal_12_davys_grey"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_10"
            android:text="For further information, refer to our "
            app:layout_constraintStart_toStartOf="@+id/view35"
            app:layout_constraintTop_toBottomOf="@+id/view35" />

        <TextView
            android:id="@+id/termsOfUse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Terms of use"
            style="@style/roboto_medium_12_red_light"
            app:layout_constraintBottom_toBottomOf="@+id/textView168"
            app:layout_constraintStart_toEndOf="@+id/textView168"
            app:layout_constraintTop_toTopOf="@+id/textView168" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>