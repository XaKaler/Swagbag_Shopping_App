<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:paddingVertical="@dimen/dimen_10"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/textView147"
        android:textColor="@color/day_dark_liver_night_white"
        style="@style/roboto_normal_16_davys_grey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Showing: "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:textColor="@color/day_dark_liver_night_Spanish_Gray"
        android:id="@+id/textView150"
        style="@style/roboto_medium_18_davys_grey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="All Orders"

        app:layout_constraintBottom_toBottomOf="@+id/textView147"
        app:layout_constraintStart_toEndOf="@+id/textView147"
        app:layout_constraintTop_toTopOf="@+id/textView147" />

    <TextView
        android:id="@+id/textView154"
        style="@style/roboto_medium_14_davys_grey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rec_outline_5"
        android:drawableStart="@drawable/ic_filter_fill"
        android:drawablePadding="@dimen/dimen_10"
        android:paddingHorizontal="@dimen/dimen_15"
        android:paddingVertical="@dimen/dimen_7"
        android:text="@string/filter"
        android:textColor="@color/day_dark_liver_night_white"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>