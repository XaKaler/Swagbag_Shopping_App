<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SwagbagShoppingApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/steel_teal_500</item>
        <item name="colorPrimaryVariant">@color/steel_teal_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/sky_blue_200</item>
        <item name="colorSecondaryVariant">@color/sky_blue_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/main_bg_color</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>

       <!-- <item name="iconTint">@color/white</item>-->

        <!-- Customize your theme here. --><!--
        <item name="actionOverflowButtonStyle">@style/IconOverFlow</item>-->
    </style>


    <style name="ToolbarColoredBackArrow" parent="AppTheme.ActionBar">
        <item name="android:textColorSecondary">@color/white</item>
    </style>


    <style name="IconOverFlow" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/white</item>
    </style>

    <style name="AppTheme.ActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        ...
        <item name="actionMenuTextColor">@color/white</item>
        ...
    </style>

